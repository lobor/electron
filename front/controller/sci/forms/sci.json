{
	"schema": {
		"type": "object",
		"properties": {
			"name": {
				"type": "string",
				"title": "Nom de la SCI / Propriétaire",
				"required":true
			},
			"rcs": {
				"type": "string",
				"title": "RCS",
				"required":true
			},
			"date_immatriculation": {
				"type": "string",
				"format": "datepicker",
				"title": "Date d'immatriculation",
				"required":true
			},
			"capital": {
				"type": "number",
				"title": "Capital Social",
				"required":true,
				"validationMessage": {
		      		"302": "Ce champs est requis"
			    }
			},
			"associes":{
				"type": "array",
				"title": "Associés",
				"items": {
					"type": "object",
					"properties": {
						"_id": {
							"title": "id",
							"type": "string"
						},
						"nom": {
							"title": "Nom",
							"type": "string"
						},
						"prenom": {
							"title": "Prenom",
							"type": "string"
						},
						"address": {
							"title": "N° et rue",
							"type": "string"
						},
						"cp": {
							"title": "Code postale",
							"type": "number"
						},
						"city": {
							"title": "Ville",
							"type": "string"
						},
						"birthday": {
							"title": "Date de naissance",
							"type": "string"
						},
						"city_birthday": {
							"title": "Lieu de naissance",
							"type": "string"
						},
						"np_part": {
							"title": "Nombre de part",
							"type": "number"
						},
						"percent_part": {
							"title": "Pourcentage des parts",
							"type": "number"
						}
					},
					"required": [
						"name",
						"comment"
					]
				}
			},
			"naf": {
				"type": "string",
				"title": "Code NAF",
				"required":true
			},
			"adress": {
				"type": "string",
				"title": "N° et rue",
				"required":true
			},
			"cp": {
				"type": "number",
				"title": "Code postale",
				"required":true
			},
			"city": {
				"type": "string",
				"title": "Ville",
				"required":true
			},
			"nb_part": {
				"type": "number",
				"title": "Nombre de parts totals",
				"required":true
			}
		}
	},
	"form": [
		{
			"type": "section",
			"htmlClass": "column",
			"items": [
				{
					"key": "name",
					"htmlClass": "col-md row md-default-theme"
				},
				{
						"key": "rcs",
						"htmlClass": "col-md row md-default-theme"
				},
				{
						"key": "date_immatriculation",
						"format": "yyyy-mm-dd",
						"type":"datepicker",
						"htmlClass":"md-default-theme"
				},
				{
						"key": "capital",
						"htmlClass": "col-md row md-default-theme"
				},
				{
						"key": "naf",
						"htmlClass": "col-md row md-default-theme"
				},
				{
						"key": "nb_part",
						"htmlClass": "col-md row md-default-theme"
				}
			]
		},
		{
			"type": "section",
			"htmlClass": "column",
			"items": [
				{
					"key": "adress",
					"htmlClass": "col-md row md-default-theme"
				},
				{
					"key": "cp",
					"htmlClass": "col-md row md-default-theme"
				},
				{
					"key": "city",
					"htmlClass": "col-md row md-default-theme"
				}
			]
		},
		{
			"key": "associes",
			"add": "Ajouter",
			"htmlClass": "row",
			"labelHtmlClass":"column",
			"style": {
				"add": "btn-success"
			},
			"items": [
				{
					"type": "section",
					"htmlClass": "row",
					"items": [
						{
							"type": "section",
							"htmlClass": "column",
							"items": [
								{
									"key": "associes[].nom",
									"htmlClass": "col-md row md-default-theme"
								},
								{
									"key": "associes[].prenom",
									"htmlClass": "col-md row md-default-theme"
								}
							]
						},
						{
							"type": "section",
							"htmlClass": "column",
							"items": [
								{
									"key": "associes[].address",
									"htmlClass": "col-md row md-default-theme"
								},
								{
									"key": "associes[].cp",
									"htmlClass": "col-md row md-default-theme"
								},
								{
									"key": "associes[].city",
									"htmlClass": "col-md row md-default-theme"
								}
							]
						},
						{
							"type": "section",
							"htmlClass": "column",
							"items": [
								{
										"key": "associes[].birthday",
										"format": "dd-mm-yyyy",
										"htmlClass": "col-md row md-default-theme",
										"type":"datepicker"
								},
								{
									"key": "associes[].city_birthday",
									"htmlClass": "col-md row md-default-theme"
								}
							]
						},
						{
							"type": "section",
							"htmlClass": "column",
							"items": [
								{
									"key": "associes[].np_part",
									"htmlClass": "col-md row md-default-theme"
								},
								{
									"key": "associes[].percent_part",
									"htmlClass": "col-md row md-default-theme"
								}
							]
						}
					]
				}
			]
		},
		{
			"type": "submit",
			"title": "Save"
		}
	]
}
